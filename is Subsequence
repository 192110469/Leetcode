class Solution {
    public boolean isSubsequence(String s, String t) {
        int len1=0,len2=0;
        while(len1<s.length()&&len2<t.length()){
            if(s.charAt(len1)==t.charAt(len2)){
                len1++;
            }
            len2++;
        }
        return len1==s.length();
    }
}
